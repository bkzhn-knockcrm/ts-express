{
    "compilerOptions": {
        "lib": ["es2021"],
        "module": "Node16",
        "target": "es2021",
        
        "outDir": "lib",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "moduleResolution": "node",

        "forceConsistentCasingInFileNames": true,
    },
    "include": [
        "src"
    ]
}